{
  "name": "@react-universal/vite-plugin",
  "version": "1.0.0",
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./src/index.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "src/index.ts",
  "scripts": {
    "build": "run -T tsup src/index.ts",
    "dev": "run esbuild src/index.ts --outfile=dist/index.js --format=cjs --log-level=silent"
  },
  "dependencies": {
    "@pigment-css/react": "^0.0.28",
    "@pigment-css/vite-plugin": "^0.0.30",
    "@react-universal/core": "workspace:*"
  },
  "devDependencies": {
    "esbuild": "^0.24.2",
    "react": "18.3.1",
    "vite": "^5.4.11"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "vite": "^5.0.0 || ^6.0.0"
  }
}
