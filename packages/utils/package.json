{
  "name": "@react-universal/utils",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "react-native": "src/index.ts",
  "license": "MIT",
  "dependencies": {
    "@github/memoize": "^1.1.5"
  },
  "scripts": {
    "build": "tsup src/index.ts src/*.ts"
  },
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "react-native": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "source": "./src/*.ts",
      "react-native": "./src/*.ts",
      "import": {
        "types": "./dist/*.d.mts",
        "default": "./dist/*.mjs"
      },
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    },
    "./package.json": "./package.json"
  }
}
